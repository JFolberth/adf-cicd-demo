			"storageaccountsecretname": {
				"type": "String",
				"defaultValue": "storageaccount-adf-demo"
			}
		},
		"annotations": [],
		"type": "AzureBlobStorage",
		"typeProperties": {
			"connectionString": "DefaultEndpointsProtocol=https;AccountName=stadfdemodev;EndpointSuffix=core.windows.net;",
			"accountKey": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVaultLinkedService",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().storageaccountsecretname",
					"type": "Expression"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}